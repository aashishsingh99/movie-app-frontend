};\n\n  // const body = JSON.stringify({name,email,password,role})\n  // console.log(body.name)\n  try {\n    // const res = await api.post('/users', formData);\n    //const res1 = await api.post('/profile', profileData);\n    const res = await axios.post('/api/users', formData, config);\n    // const res={data:'hi'}\n    dispatch({\n      type: REGISTER_SUCCESS,\n      payload: res.data,\n    });\n    dispatch(loadUser());\n  } catch (err) {\n    \n\n    dispatch({\n      type: REGISTER_FAIL,\n    });\n  }\n};\n\n// Login User\nexport const login = (body) => async dispatch => {\n  const config = {\n    headers: {\n      'Content-Type': 'application/json',\n    },\n  };\n  \n\n  try {\n    // const res = await api.post('/auth', body);\n    console.log('hi')\n    const res = await axios.post('/api/auth', body, config);\n\n    // const res={data:'hi'}\n    //console.log(res.data)\n    await dispatch({\n      type: LOGIN_SUCCESS,\n      payload: res.data,\n    });\n\n   await dispatch(loadUser());\n  } catch (err) {\n    console.log(err);\n   \n    dispatch({\n      type: LOGIN_FAIL,\n    });\n  }\n};\n\n// Logout\nexport const logout = () => ({ type: CLEAR_PROFILE, type: LOGOUT });\n","/home/lenovo/Desktop/projects/reservation-frontend/client/src/actions/alert.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/reducers/index.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/actions/types.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/reducers/auth.js",["160"],"import {\n  REGISTER_SUCCESS,\n  //REGISTER_FAIL,\n  USER_LOADED,\n  AUTH_ERROR,\n  LOGIN_SUCCESS,\n  //=LOGIN_FAIL,\n  LOGOUT,\n  \n \n  ACCOUNT_DELETED\n} from '../actions/types';\n\nconst initialState = {\n  token: localStorage.getItem('token'),\n  isAuthenticated: null,\n  loading: true,\n  user: null,\n  role: null,\n};\n\nfunction authReducer(state = initialState, action) {\n  const { type, payload } = action;\n\n  switch (type) {\n    case USER_LOADED:\n      return {\n        ...state,\n        isAuthenticated: true,\n        loading: false,\n        user: payload,\n        role: payload.role,\n      };\n    case REGISTER_SUCCESS:\n    case LOGIN_SUCCESS:\n      localStorage.setItem('token',payload.token)\n      return {\n        ...state,\n        token: payload.token,// We need to why this works\n        isAuthenticated: true,\n        loading: false,\n  \n      };\n\n    case AUTH_ERROR:\n    case LOGOUT:\n      localStorage.removeItem('token')\n      return {\n        ...state,\n        token: null,\n        isAuthenticated: false,\n        loading: false,\n        user: null,\n        role: null,\n      };\n    default:\n      return state;\n  }\n}\n\nexport default authReducer;\n","/home/lenovo/Desktop/projects/reservation-frontend/client/src/reducers/alert.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/components/auth/LoginComp.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/auth/Login.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/auth/Register.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/layout/Alert.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/layout/Landing.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/layout/Navbar.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/utils/setAuthToken.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/utils/api.js",[],["161","162"],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/routing/PrivateRoute.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/dashboard/Dashboard.js",["163"],"import React, { Fragment, useState } from 'react';\nimport { connect } from 'react-redux';\nimport { Link, Redirect } from 'react-router-dom';\nimport { getAllBuses } from '../../actions/bus';\n\nimport { register } from '../../actions/auth';\nimport PropTypes from 'prop-types';\n\nconst Dashboard = ({ auth: { role, user }, getAllBuses}) => {\n  const [redirect, setRedirect] = useState({\n    redirectToNewPage: false,\n  });\n\n  if (redirect.redirectToNewPage) {\n    return <Redirect to='/buses' />;\n  }\n const click=()=>{\n   getAllBuses();\n   setRedirect({ ...redirect, redirectToNewPage: true });\n  \n }\n  if (role === 'user') {\n    return (\n      <Fragment>\n        <div> Hi </div>\n        <div>\n          <Link to='/findBus'>\n            <button className='btn btn-primary'> Find Bus </button>\n          </Link>\n        </div>\n      </Fragment>\n    );\n  } else {\n    return (\n      <Fragment>\n        Hi (admin)\n        <div>\n        <Link to='/buses'>\n          <button className='btn btn-primary' onClick={click}>\n            {' '}\n            View All Buses{' '}\n          </button>\n          </Link>\n\n          <Link to='/addBus'>\n            <button className='btn btn-primary'> Add Bus </button>\n          </Link>\n        </div>\n      </Fragment>\n    );\n  }\n};\n\nDashboard.propTypes = {\n  auth: PropTypes.object.isRequired,\n  getAllBuses: PropTypes.func.isRequired,\n};\nconst mapStateToProps = state => ({\n  auth: state.auth,\n});\nexport default connect(mapStateToProps, {getAllBuses})(Dashboard);\n","/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/bus/AllBuses.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/components/errors/NotAuth.js",[],"/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/bus/FindBus.js",["164","165","166","167","168","169"],"import React, { Fragment, useState } from 'react';\nimport { connect } from 'react-redux';\nimport { Link, Redirect } from 'react-router-dom';\nimport { setAlert } from '../../actions/alert';\nimport { findBus } from '../../actions/bus';\nimport NotAuth from '../../components/errors/NotAuth';\nimport PropTypes from 'prop-types';\nimport FindBusComp from '../../components/bus/FindBusComp';\nconst FindBus = ({ auth: { role }, findBus }) => {\n  const [formData, setFormData] = useState({\n    source: '',\n    destination: '',\n    date: '',\n    redirectToNewPage: false,\n  });\n\n  const { source, destination, date } = formData;\n  if (formData.redirectToNewPage) {\n    return <Redirect to='/buses' />;\n  }\n  const onChange = e =>\n    setFormData({ ...formData, [e.target.name]: e.target.value });\n\n  const onSubmit = async e => {\n    e.preventDefault();\n    findBus(formData);\n    setFormData({ ...formData, redirectToNewPage: true });\n  };\n  if (role === 'user') {\n    return (\n      <FindBusComp formData={formData} onChange={onChange} onSubmit={onSubmit}>\n        {' '}\n      </FindBusComp>\n    );\n  } else {\n    return <NotAuth allowed='User' />;\n  }\n};\nFindBus.propTypes = {\n  findBus: PropTypes.func.isRequired,\n  auth: PropTypes.object.isRequired,\n};\n\nconst mapStateToProps = state => ({\n  auth: state.auth,\n});\n\nexport default connect(mapStateToProps, { findBus })(FindBus);\n","/home/lenovo/Desktop/projects/reservation-frontend/client/src/containers/bus/AddBus.js",["170","171","172"],"import React, { Fragment, useState } from 'react';\nimport { connect } from 'react-redux';\nimport { Link, Redirect } from 'react-router-dom';\nimport { setAlert } from '../../actions/alert';\nimport { addBus } from '../../actions/bus';\nimport NotAuth from '../../components/errors/NotAuth';\nimport PropTypes from 'prop-types';\nimport AddBusComp from '../../components/bus/AddBusComp';\nconst AddBus = ({ auth: { role }, addBus, bus: { error } }) => {\n  const [formData, setFormData] = useState({\n    source: '',\n    destination: '',\n    startTime: '',\n    reachTime: '',\n    date: '',\n    redirectToNewPage: false,\n  });\n\n  if (formData.redirectToNewPage) {\n    return <Redirect to='/buses' />;\n  }\n  const onChange = e =>\n    setFormData({ ...formData, [e.target.nam